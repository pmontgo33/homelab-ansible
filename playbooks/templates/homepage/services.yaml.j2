### MEDIA ###

- Media Servers:
    - Jellyfin:
        href: https://jellyfin.{{ cloudflare_domain }}
        siteMonitor: https://jellyfin.{{ cloudflare_domain }}
        description: Movie and TV Show Library
        icon: sh-jellyfin.svg
        widget:
          type: jellyfin
          url: https://jellyfin.{{ cloudflare_domain }}
          key: {{jellyfin_key}}
        #  enableBlocks: true # optional, defaults to false
          enableNowPlaying: true # optional, defaults to true
          enableUser: true # optional, defaults to false
          showEpisodeNumber: true # optional, defaults to false
          expandOneStreamToTwoRows: false # optional, defaults to true
    - Immich:
        href: https://immich.{{ cloudflare_domain }}
        siteMonitor: https://immich.{{ cloudflare_domain }}
        description: Photo Library
        icon: sh-immich.svg
        widget:
          type: immich
          url: https://immich.{{ cloudflare_domain }}
          key: {{immich_key}}
          version: 2 # optional, default is 1
    - Audiobookshelf:
        href: https://audiobooks.{{ cloudflare_domain }}
        siteMonitor: https://audiobooks.{{ cloudflare_domain }}
